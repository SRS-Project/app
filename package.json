{
  "name": "@srs-project/app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "precommit": "lint-staged",
    "clean": "rm -rf dist",
    "start": "electron .",
    "dev": "cross-env NODE_ENV=development START_HOT=1 npm run dev-renderer",
    "dev-main": "webpack --mode development --config webpack.main.config.js && electron ./dist/main.js",
    "dev-renderer": "webpack-dev-server --mode development --config webpack.renderer.dev.config.js",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/SRS-Project/app.git"
  },
  "keywords": [
    "srs"
  ],
  "author": "SRS Project",
  "license": "MIT",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-transition-group": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-syntax-decorators": "^7.0.0-beta.46",
    "@babel/plugin-syntax-jsx": "^7.0.0-beta.46",
    "@babel/plugin-syntax-typescript": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-test-renderer": "^16.0.1",
    "awesome-typescript-loader": "^5.0.0",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cross-env": "^5.1.4",
    "electron": "^1.8.6",
    "electron-compile": "^6.4.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.5",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "react-hot-loader": "^4.1.2",
    "react-test-renderer": "^16.3.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
